{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "cover": {
            "type": "object",
            "properties": {
                "filename": {
                    "type": "string"
                },
                "title": {
                    "type": "object",
                    "properties": {
                        "main": {
                            "type": "string"
                        },
                        "chapter": {
                            "type": "integer"
                        },
                        "subject": {
                            "type": "string"
                        },
                        "superimposed": {
                            "type": "boolean"
                        }
                    }
                },
                "level": {
                    "type": "integer"
                },
                "links": {
                    "type": "object",
                    "properties": {
                        "tutorial": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string"
                                },
                                "link": {
                                    "type": "string"
                                }
                            }
                        },
                        "youtube": {
                            "type": "string"
                        }
                    }
                },
                "required": {
                    "type": "object",
                    "properties": {
                        "equipment": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "quantity": {
                                            "type": "integer"
                                        },
                                        "object": {
                                            "type": "string"
                                        }
                                    }
                                }
                            ]
                        },
                        "skills": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "title": {
                                            "type": "string"
                                        },
                                        "link": {
                                            "type": "string"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "introduction": {
            "type": "object",
            "properties": {
                "title": {
                    "type": "string"
                },
                "content": {
                    "type": "array",
                    "items": [
                        {
                            "type": "string"
                        }
                    ]
                }
            }
        },
        "tutorials": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "properties": {
                        "title": {
                            "type": "string"
                        },
                        "introduction": {
                            "type": "string"
                        },
                        "sections": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "title": {
                                            "type": "string"
                                        },
                                        "steps": {
                                            "type": "array",
                                            "items": [
                                                {
                                                    "type": "string"
                                                }
                                            ]
                                        }
                                    }
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "title": {
                                            "type": "string"
                                        },
                                        "steps": {
                                            "type": "array",
                                            "items": [
                                                {
                                                    "type": "string"
                                                }
                                            ]
                                        }
                                    }
                                }
                            ]
                        }
                    }
                },
                {
                    "type": "object",
                    "properties": {
                        "title": {
                            "type": "string"
                        },
                        "introduction": {
                            "type": "string"
                        },
                        "sections": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "title": {
                                            "type": "string"
                                        },
                                        "steps": {
                                            "type": "array",
                                            "items": [
                                                {
                                                    "type": "string"
                                                }
                                            ]
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            ]
        }
    }
}